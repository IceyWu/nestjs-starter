    "@casl/ability": "^6.7.3",
    "@casl/prisma": "^1.5.1",
    "@iceywu/utils": "^0.0.49",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
