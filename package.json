    "sharp": "^0.33.0",
    "simple-git": "^3.24.0",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^5.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
