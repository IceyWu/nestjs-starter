    "@prisma/client": "^6.4.1",
    "@scalar/api-reference": "^1.25.121",
    "@scalar/nestjs-api-reference": "^0.3.184",
    "@types/ali-oss": "^6.16.11",
    "ali-oss": "^6.22.0",
    "argon2": "^0.41.1",
    "blurhash": "^2.0.5",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "changelogen": "^0.5.7",
